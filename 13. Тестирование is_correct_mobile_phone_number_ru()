from yandex_testing_lesson import is_correct_mobile_phone_number_ru




n={"+7(900)1234567":True, "+7 999 123-45-67":True, "12 999 123-45-67":False, "+7 999 12)((3-45-67":False, "+7 999 123-45":False}
f=0
for i in n:
    if is_correct_mobile_phone_number_ru(i) != n[i]:
        f=1

if f==1:
    print("NO")
else:
    print("YES")
