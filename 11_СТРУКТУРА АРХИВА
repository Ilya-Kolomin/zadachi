import zipfile
res=[]
def look(x,intend,target=""):
    for i in x:
        if (i.startswith(target) and i not in res):
            res.append(i)
            if(not i.endswith('/')):
                print(" " * intend + i[len(target)::])
            if(i.endswith('/')):
                print(" " * intend + i[len(target):-1:])
                look(x,intend+2,i)




with zipfile.ZipFile('input.zip','r') as myzip:
    look(myzip.namelist(),0)


